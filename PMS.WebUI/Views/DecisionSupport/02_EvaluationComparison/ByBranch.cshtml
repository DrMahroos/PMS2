
@using PMS.WebUI.Resources


<div class="clear"></div>
<label for="rpTitle" style="display:none" id="rpTitle" langRef="Rep_AttendanceAndLeaveS">@SystemResource.Men_SalesComparison</label>
<div class="row">
    <div class="clear"></div>
    <div class="col-xs-12 dir" id="effects">
        <div style="height:5px"></div>
        <div class="clear"></div>
        <form class="col-xs-12">
            <div class="col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Eng_fld_Region</label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 120px;">
                            <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtRegionID">
                            <input type="text" class="form-control input-sm" id="txtRegionCode" name="txtProjectEngineerRP" disabled>
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" id="btnRegionID" name="btnProjectEngineerRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width:25.5%">
                            <input type="text" class="form-control inpt-sm" id="txtRegionDes" disabled>
                        </div>
                    </div>
                </div>


                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.PManag_FromDate</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="date" class="form-control input-sm" id="FromDate" name="txtProjectEngineerRP">
                        </div>
                        <label class="col-sm-2 col-xs-12 control-label text-right" style=" width: 79px;">@SystemResource.ResM_fld_laborrequest_todate</label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 166px;">
                            <input type="date" class="form-control inpt-sm" id="ToDate">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_ProjectStatuscustomerclass</label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 120px;">
                            <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtCustClass">
                            <input type="text" class="form-control input-sm" name="txtProjectEngineerRP" id="txtCustClassCod" disabled>
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" name="btnProjectEngineerRP" id="btnCustClassID">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width:25.5%">
                            <input type="text" class="form-control inpt-sm" name="btnProjectEngineerRP" id="txtCustClassDes" disabled>
                        </div>
                    </div>
                </div>

                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Eng_Fld_projectexpscopeclass</label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 120px;">
                            <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtScopeCalID">
                            <input type="text" class="form-control input-sm" name="txtProjectEngineerRP" id="txtScopeCalCod" disabled>
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" name="btnProjectEngineerRP" id="btnScopeCalID">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width:25.5%">
                            <input type="text" class="form-control inpt-sm" id="txtScopeCalDes" disabled>
                        </div>
                    </div>
                </div>

                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_scope</label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 120px;">
                            <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtScopeID">
                            <input type="text" class="form-control input-sm" name="txtProjectEngineerRP" id="txtScopeCode" disabled>
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" name="btnProjectEngineerRP" id="butScopeID">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width:25.5%">
                            <input type="text" class="form-control inpt-sm" id="txtScopeDes" disabled>
                        </div>
                    </div>
                </div>
                <div class="col-md-5 popsection">
                    <div class="col-md-12">
                        <div class="col-md-6 col-xs-5">

                            <input type="radio" checked name="price" class="dailwork_input" id="RedAllProject" />
                            <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_allproject</label>
                        </div>
                        <div class="col-md-6 col-xs-5">
                            <input type="radio" name="price" class="dailwork_input" id="RedNewProjects" />
                            <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_nonrepair</label>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="col-md-12 col-xs-5">
                            <input type="radio" checked name="price" class="dailwork_input" id="RedAdditionalWork" />
                            <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_repair</label>
                        </div>
                    </div>

                </div>
</form>
    </div>
</div>



<script src="~/ClientApp/Components/DecisionSupport/02_EvaluationComparison/ByBranch.js"></script>