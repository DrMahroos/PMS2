

@using PMS.WebUI.Resources


<div class="clear"></div>
<label for="rpTitle" style="display:none" id="rpTitle" langRef="Rep_AttendanceAndLeaveS">@SystemResource.Men_ProductionComparison</label>
<div class="row">
    <div class="clear"></div>
    <div class="col-xs-12 dir" id="effects">
        <div style="height:5px"></div>
        <div class="clear"></div>
        <form class="col-xs-12">
            <div class="col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-7">
                            <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_fld_Region</label>
                            <div class="col-md-2 col-sm-4 col-xs-5">
                                <input type="text" class="form-control input-sm" id="txtRegionID" name="Region2"disabled>
                            </div>
                            <div class="info-btn">
                                <button type="button" class="btn btn-warning" id="btnRegionID">
                                    <i class="glyphicon glyphicon-search"></i>
                                </button>
                            </div>
                            <div class="col-md-5 col-sm-4 col-xs-5">
                                <input type="text" class="form-control inpt-sm" id="txtRegionDes" name="Region4"disabled>
                            </div>
                        </div>

                        <div class="col-md-5 col-xs-5 text-center">
                            <label style="margin: 6px 5px;" class="labelpopup">@SystemResource.Ger_Rep_fld_Included</label>
                        </div>


                    </div>
                </div>


                <div class="form-horizontal"style="margin:-5px;">
                    <div class="form-group">
                        <div class="col-md-7">
                          
                            <div class="col-md-12" style="padding:0px;margin:6px">
                                <label for="txtEngCode" class="col-sm-3 col-xs-12 control-label text-right"> @SystemResource.Fld_Year </label>
                                <div class="col-md-1 col-sm-4 col-xs-5" style="width: 100px;">
                                  
                                    <input type="text" class="form-control input-sm" id="txtyer" name="txtyear">

                                </div>
                             
                            </div>

                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_customerclass</label>
                                <div class="col-md-2 col-sm-4 col-xs-5">
                                       
                                    <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtCustClass">
                                    <input type="text" class="form-control input-sm" name="txtScopeCalCod" id="txtCustClassCod"disabled>
                                </div>
                                <div class="info-btn">
                                    <button type="button" class="btn btn-warning" id="btnCustClassID">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                                <div class="col-md-5 col-sm-4 col-xs-5">
                                    <input type="text" class="form-control inpt-sm" id="txtCustClassDes"disabled>
                                </div>

                            </div>
                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_scopeclass</label>
                                <div class="col-md-2 col-sm-4 col-xs-5">
                                    <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtScopeCalID">
                                    <input type="text" class="form-control input-sm" id="txtScopeCalCod" name="txtScopeCalCod"disabled>
                                </div>
                                <div class="info-btn">
                                    <button type="button" class="btn btn-warning" id="btnScopeCalID">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                                <div class="col-md-5 col-sm-4 col-xs-5">
                                    <input type="text" class="form-control inpt-sm" id="txtScopeCalDes"disabled>
                                </div>
                            </div>

                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_scope</label>
                                <div class="col-md-2 col-sm-4 col-xs-5">
                                    <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtScopeID">
                                    <input type="text" class="form-control input-sm" id="txtScopeCode" name="txtScopeCode"disabled>
                                </div>
                                <div class="info-btn">
                                    <button type="button" class="btn btn-warning" id="butScopeID" name="ScopeID">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                                <div class="col-md-5 col-sm-4 col-xs-5">
                                    <input type="text" class="form-control inpt-sm" id="txtScopeDes"disabled>
                                </div>
                            </div>

                        </div>

                        <div class="col-md-5 popsection">
                            <div class="col-md-12">
                                <div class="col-md-6 col-xs-5">

                                    <input type="radio" checked name="price" class="dailwork_input" id="RedAllProject" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_allproject</label>
                                </div>
                                <div class="col-md-6 col-xs-5">
                                    <input type="radio" name="price" class="dailwork_input" id="RedNonRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_nonrepair</label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="col-md-12 col-xs-5">
                                    <input type="radio" checked name="price" class="dailwork_input" id="RedRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_repair</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @*<div class="form-horizontal">
                    <div class="col-md-7">
                        <div class="form-group">
                            <div class="col-md-12" style="padding: 0px;margin: 5px 0px;">
                                <label for="txtEngCode" class="col-sm-3 col-xs-12 control-label text-right"> @SystemResource.Ger_Rep_fld_Included </label>

                                <div class="col-md-2 col-xs-5" style="width: 160px;">
                                    <input type="radio" name="repar3" class="dailwork_input" id="RedAllProject" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_allproject</label>
                                </div>

                                <div class="col-md-2 col-xs-5" style="width: 160px;">
                                    <input type="radio" name="repar3" class="dailwork_input" id="RedNonRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_nonrepair</label>
                                </div>


                                <div class="col-md-2 col-xs-5" style="width: 136px;">
                                    <input type="radio" name="repar3" class="dailwork_input" id="RedRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_repair</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>*@
            </div>
        </form>
    </div>
</div>


<script src="~/ClientApp/Components/DecisionSupport/01_BranchComparison/ProductionComparison.js"></script>