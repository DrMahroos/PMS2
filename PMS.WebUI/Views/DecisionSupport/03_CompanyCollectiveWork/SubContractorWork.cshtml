

@using PMS.WebUI.Resources


<div class="clear"></div>
<label for="rpTitle" style="display:none" id="rpTitle" langRef="Rep_AttendanceAndLeaveS">@SystemResource.Men_SalesComparison</label>
<div class="row">
    <div class="clear"></div>
    <div class="col-xs-12 dir" id="effects">
        <div style="height:5px"></div>
        <div class="clear"></div>
        <form class="col-xs-12">
            <div class="col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-7">


                            <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_fld_Region</label>
                            <div class="col-md-2 col-sm-4 col-xs-5">
                                <input type="text" class="form-control input-sm" id="txtRegionID" name="Region2" disabled>
                            </div>
                            <div class="info-btn">
                                <button type="button" class="btn btn-warning" id="btnRegionID">
                                    <i class="glyphicon glyphicon-search"></i>
                                </button>
                            </div>
                            <div class="col-md-5 col-sm-4 col-xs-5">
                                <input type="text" class="form-control inpt-sm" id="txtRegionDes" name="Region4" disabled>
                            </div>
                        </div>

                        <div class="col-md-5 col-xs-5 text-center">
                            <label style="margin: 6px 0px;" class="labelpopup">@SystemResource.Ger_Rep_fld_Included</label>
                        </div>


                    </div>
                </div>


                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-7">
                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label for="txtEngCode" class="col-sm-3 col-xs-12 control-label text-right"> @SystemResource.PManag_FromDate </label>
                                <div class="col-md-1 col-sm-4 col-xs-5" style="width: 151px;">
                                    <input type="date" class="form-control input-sm addable editable" name="EngCode" id="FromDate">
                                </div>
                                <label for="txtEngCode" class="col-sm-1 col-xs-12 control-label text-right" style="width: 90px;"> @SystemResource.ResM_fld_laborrequest_todate </label>
                                <div class="col-md-1 col-sm-4 col-xs-5" style="width: 151px;">
                                    <input type="date" class="form-control input-sm addable editable" name="EngCode" id="ToDate">
                                </div>
                            </div>





                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label for="txtEngCode" class="col-sm-3 col-xs-12 control-label text-right"> @SystemResource.Eng_Rep_Dailyworkprod_customerclass@*Fld_VendorID*@ </label>
                                <div class="col-md-2 col-sm-4 col-xs-5">
                                    <input type="hidden" class="form-control input-sm" id="txtCustClass" name="txtCustClass">
                                    <input type="text" class="form-control input-sm" id="txtCustClassCod" name="txtCustClassCod" disabled>

                                </div>
                                <div class="info-btn">
                                    <button type="button" class="btn btn-warning" id="btnCustClassID">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                                <div class="col-md-5 col-sm-4 col-xs-5">
                                    <input type="text" class="form-control inpt-sm" id="txtCustClassDes" name="txtCustClassDes" disabled>
                                </div>

                            </div>


                            <div class="col-md-12" style="padding:0px;margin:5px">
                                <label class="col-sm-3 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_scopeclass</label>
                                <div class="col-md-2 col-sm-4 col-xs-5">
                                    <input type="hidden" class="form-control input-sm" name="txtProjectEngineerID" id="txtScopeCalID">
                                    <input type="text" class="form-control input-sm" id="txtScopeCalCod" name="txtScopeCalCod" disabled>
                                </div>
                                <div class="info-btn">
                                    <button type="button" class="btn btn-warning" id="btnScopeCalID">
                                        <i class="glyphicon glyphicon-search"></i>
                                    </button>
                                </div>
                                <div class="col-md-5 col-sm-4 col-xs-5">
                                    <input type="text" class="form-control inpt-sm" id="txtScopeCalDes" disabled>
                                </div>
                            </div>

                        </div>

                        <div class="col-md-5 popsection">
                            <div class="col-md-12">
                                <div class="col-md-6 col-xs-5">

                                    <input type="radio" checked name="price" class="dailwork_input" id="RedAllProject" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_allproject</label>
                                </div>
                                <div class="col-md-6 col-xs-5">
                                    <input type="radio" name="price" class="dailwork_input" id="RedNonRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_nonrepair</label>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="col-md-12 col-xs-5">
                                    <input type="radio" checked name="price" class="dailwork_input" id="RedRepair" />
                                    <label style="margin: 3px 16px;">@SystemResource.Eng_Rep_Dailyworkprod_repair</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="form-horizontal">
                <div class="form-group">

                    <div class="col-md-6 popsection" style="margin-top:20px;">
                        <div class="col-md-12">
                            <div class="col-md-4 col-xs-4">

                                <label class="pouplabel">@SystemResource.Rpt_Grouping</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input type="radio" checked name="reparir1" class="dailwork_input" id="RedByScope" />
                                <label class="pouplabel">@SystemResource.ByScope</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input name="reparir1" type="radio" class="dailwork_input" id="RedByBranch" />
                                <label class="pouplabel">@SystemResource.Cust_BraCode</label>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-4 col-xs-4">

                                <label class="pouplabel">@SystemResource.Resm_reporttype</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input type="radio" checked name="reptype" class="dailwork_input" id="RedSummary" />
                                <label class="pouplabel">@SystemResource.Spec_summary</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input name="reptype" type="radio" class="dailwork_input" id="RedDetail" />
                                <label class="pouplabel">@SystemResource.Spec_Detail</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*<div class="form-horizontal">
                <div class="form-group">

                    <div class="col-md-6 popsection" style="margin-top:20px;">
                        <div class="col-md-12">
                            <div class="col-md-4 col-xs-4">

                                <label class="pouplabel">@SystemResource.Resm_reporttype</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input type="radio" name="reparir1" class="dailwork_input" id="RedSummary" />
                                <label class="pouplabel">@SystemResource.Spec_summary</label>
                            </div>
                            <div class="col-md-4 col-xs-4">
                                <input name="reparir1" type="radio" class="dailwork_input" id="RedDetail" />
                                <label class="pouplabel">@SystemResource.Spec_Detail</label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>*@
        </form>
    </div>
</div>


<script src="~/ClientApp/Components/DecisionSupport/03_CompanyCollectiveWork/SubContractorWork.js"></script>
