@using PMS.WebUI.Resources
@{
    ViewBag.ScreenKey = "UsersSetting";
    ViewBag.Title = @SystemResource.Men_UsersSetting;
   Layout = "~/Views/Shared/Layout.cshtml";
}
} 
<div class="row">
    <div class="color1">
        @Html.ControlsButtons()
    </div>
    <link href="~/css/users.css" rel="stylesheet" />
    <link href="~/css/removStyelCeckedbokx.css" rel="stylesheet" />
    @*<link href="~/css/mainUsers.css" rel="stylesheet" />*@
    <div class="clear"></div>

  

            <div id="farestdive" class="dir col-xs-12">
                <div style="height:5px"></div>
                <div class="clear"></div>
                <div class="col-xs-12">
                    <div class="form-horizontal">
                        <div class="col-xs-12 form-group">
                            <label for="txtUSER_CODE" class="col-md-2 col-xs-12 control-label text-left">@SystemResource.URS_UserCode </label>
                            <div class="col-md-2 col-xs-10">
                                @*<input type="hidden" name="USER_CODE" id="txtUSER_CODE">*@
                                <input type="text" class="form-control input-sm addable xeditable" name="USER_CODE" id="txtUSER_CODE">
                            </div>
                            <div class="col-xs-1">
                                <button type="button" class="src-btn btn btn-warning xaddable" id="btnSearchUserCode" name="btnSearchUserCode">
                                    <i class="fa fa-search"></i>
                                </button>
                            </div>
                            <div class="col-sm-1 hidden-xs" style="padding:0%;margin-left: 80px;">
                                <label for="txtStatus" class="d-send col-xs-12 control-label libel2" style="text-align:left;"> @SystemResource.URS_Active  </label>
                            </div>
                            <input type="checkbox" class="col-xs-2 addable editable genralcheck libelrelation" id="chkUSER_ACTIVE" name="USER_ACTIVE" value="">

                            <div class="col-sm-2 hidden-xs" style="padding:0%;">
                                <label for="txtStatus" class="d-send col-xs-12 control-label textrelation" style="text-align:left;"> @SystemResource.URS_KeepRelation   </label>
                            </div>
                            <input type="checkbox" class="col-xs-2 addable editable genralcheck libelrelation" id="chkKeepRelation" name="LoginUrl">

                        </div>
                    </div>
                    <div class="form-horizontal">
                        <div class="col-xs-12 form-group">
                            <label for="txtUSER_NAME" class="col-md-2 col-xs-12 control-label text-left"> @SystemResource.URS_UserName </label>
                            <div class="col-md-4 col-xs-10">
                                <input type="text" maxlength="100" class="form-control input-sm addable editable" name="USER_NAME" id="txtUSER_NAME" disabled="disabled">
                            </div>
                            <label for="txtcopyFrom" class="col-md-2 col-xs-12 control-label text-left" style="text-align: center;"> @SystemResource.URS_CopyFrom </label>
                            <div class="col-md-2 col-xs-10">
                                <input type="text" maxlength="100" class="form-control input-sm addable xeditable" name="ManagedBy" id="txtcopyFrom" disabled="disabled">
                            </div>
                            <div class="col-xs-1">
                                <button type="button" class="src-btn btn btn-warning addable editable" id="btnCopyFrom" name="btnCopyFrom">
                                    <i class="fa fa-search"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="form-horizontal">
                        <div class="col-xs-12 form-group">
                            <label for="txtUSER_PASSWORD" class="col-md-2 col-xs-12 control-label text-left"> @SystemResource.URS_Password  </label>
                            <div class="col-md-4 col-xs-10">
                                <input type="password" maxlength="100" class="form-control input-sm addable editable" name="USER_PASSWORD" id="txtUSER_PASSWORD" disabled="disabled">
                            </div>
                            <label for="txtUSER_PASSWORD" class="col-md-2 col-xs-12 control-label text-left"> @SystemResource.URS_Job </label>
                            <div class="col-md-4 col-xs-10">
                                <input type="text" maxlength="100" class="form-control input-sm addable editable" name="JobTitle" id="txtJobTitle" disabled="disabled">
                            </div>
                        </div>
                        <div class="col-xs-12 form-group">
                            <label for="txtUSER_PASSWORD" class="col-md-2 col-xs-12 control-label text-left"> @SystemResource.URS_City  </label>
                            <div class="col-md-4 col-xs-10">
                                <input type="text" maxlength="100" class="form-control input-sm addable editable" name="City" id="txtCity" disabled="disabled">
                            </div>

                            <label for="txtUSER_PASSWORD" class="col-md-2 col-xs-12 control-label text-left"> @SystemResource.URS_Department </label>
                            <div class="col-md-4 col-xs-10">
                                <input type="text" maxlength="100" class="form-control input-sm addable editable" name="DepartmentName" id="txtDepartmentName" disabled="disabled">
                            </div>



                        </div>
                        @*<div class="col-xs-12 form-group">
                            
                        </div>*@
                        @*<div class="col-xs-12 form-group" style="margin-bottom:4px;">
                           
                        </div>*@
                    </div>
                    <div class="col-xs-12 cutoms editable">
                        <div class="form-horizontal">
                            <div class="col-xs-12 form-group">
                                <input type="text" id="txtmodulcode" style="background-color: #d6a856;" class="col-xs-2 editable" readonly />
                                <div class="col-xs-10">
                                    <div class="check-btn">
                                        <input type="checkbox" id="chkexcute" class="col-xs-12 ckek">
                                        <label class="col-xs-1"> @SystemResource.URS_Operate </label>
                                        <input type="checkbox" id="chkadd" class="col-xs-12 ckek">
                                        <label class="col-xs-1">@SystemResource.URS_Add</label>
                                        <input type="checkbox" id="chkedit" class="col-xs-12 ckek">
                                        <label class="col-xs-1">@SystemResource.URS_Edit</label>
                                        <input type="checkbox" id="chkdelete" class="col-xs-12 ckek">
                                        <label class="col-xs-1">@SystemResource.URS_Delete</label>
                                        <input type="checkbox" id="chkview" class="col-xs-12 ckek">
                                        <label class="col-xs-1">@SystemResource.URS_Show</label>
                                        <input type="checkbox" id="chkprint" class="col-xs-12 ckek">
                                        <label class="col-xs-1">@SystemResource.URS_Print</label>
                                        <input type="checkbox" id="chkviewImg" class="col-xs-12 ckek">
                                        <label class="col-xs-2">@SystemResource.URS_ShowImg</label>
                                        <input type="checkbox" id="chkEditImg" class="col-xs-12 ckek chek3">
                                        <label class="col-xs-2 chek2">@SystemResource.URS_EditImg</label>
                                    </div>
                                    <div class="form-group col-xs-12">
                                        <div class="col-xs-3" @*style="padding-right:0.5%;width: 24%; margin-left: 98px;"*@>
                                            <input class="input-sm" type="text" id="txtcustom1" style="border-radius:7px; border:1px solid #808080" name="CUSTOM1_DESC" placeholder="Empty" readonly>
                                            <input type="checkbox" id="chkcustom1" name="CUSTOM1" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom2" style="border-radius:7px; border:1px solid #808080" name="CUSTOM2_DESC" placeholder="Empty" readonly>
                                            <input type="checkbox" id="chkcustom2" name="CUSTOM2" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom3" style="border-radius:7px; border:1px solid #808080" name="CUSTOM3_DESC" placeholder="Empty" readonly>
                                            <input type="checkbox" id="chkcustom3" name="CUSTOM3" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom4" style="border-radius:7px; border:1px solid #808080" name="CUSTOM4_DESC" placeholder="Empty" readonly>
                                            <input type="checkbox" id="chkcustom4" name="CUSTOM4" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom5" style="border-radius:7px; border:1px solid #808080" name="CUSTOM5_DESC" placeholder="Empty" readonly style="width:90%;">
                                            <input type="checkbox" id="chkcustom5" name="CUSTOM5" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom6" style="border-radius:7px; border:1px solid #808080" name="CUSTOM6_DESC" placeholder="Empty" readonly />
                                            <input type="checkbox" id="chkcustom6" name="CUSTOM6" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom7" style="border-radius:7px; border:1px solid #808080" name="CUSTOM7_DESC" placeholder="Empty" readonly />
                                            <input type="checkbox" id="chkcustom7" name="CUSTOM7" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom8" style="border-radius:7px; border:1px solid #808080" name="CUSTOM8_DESC" placeholder="Empty" readonly />
                                            <input type="checkbox" id="chkcustom8" name="CUSTOM8" hidden />
                                        </div>
                                        <div class="col-xs-3">
                                            <input class="input-sm" type="text" id="txtcustom9" style="border-radius:7px; border:1px solid #808080" name="CUSTOM9_DESC" placeholder="Empty" readonly style="width:90%;" />
                                            <input type="checkbox" id="chkcustom9" name="CUSTOM9" hidden />
                                        </div>

                                    </div>

                                    <button class="btn btn-warning editable xaddable" id="btnAddToGrid" style="position: absolute;top: 34%;left: -4%;"> @SystemResource.URS_Save</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clear"></div>
                </div>
                <style>
                    #grid {
                        height: 300px !important;
                    }

                    .jsgrid-grid-body {
                        height: 500px !important;
                    }

                    .jsgrid-grid-header.jsgrid-header-scrollbar {
                        overflow-x: scroll;
                    }

                    .jsgrid-table {
                        width: 160%;
                        overflow: scroll;
                    }

                    tr.jsgrid-header-row {
                        overflow-x: scroll;
                    }
                    .libelrelation {
                        width: 23px !important;
                        margin:0px!important;
                    }
                    .textrelation {
                     margin-left:100px
                    }
                    .libel2 {
                        margin-left: 30px;
                    }
                    .ckek {
                        margin-top: -7px !important;
                        width: 23px !important;

                    }
                    .chek2 {
                        margin: 0px 0px 0px -58px !important;
                    }
                    .chek3 {
                        margin-left: -80px!important;
                    }


                    .cutoms {
                        margin-top: 5px;
                        margin-bottom: 5px;
                    }

                    #farestdive .form-group {
                        margin-top: 5px;
                    }
                    #SubSystemsGrid .jsgrid-table {
                        width: 100%!important;
                        overflow: scroll;
                    }
                    #UserBranchGrid .jsgrid-table {
                        width: 100% !important;
                        overflow: scroll;
                    }
                </style>

                <script src="~/ClientApp/JsGrid.js"></script>
                <script src="~/ClientApp/Components/Definitions/07_SubOtherDefinition/G_USERS.js"></script>
                <table id="grid"></table>
                Sub Systems
                <table id="SubSystemsGrid"></table>
                User Branch
                <table id="UserBranchGrid"></table>
               
            </div>
    <div id="a">
        @Html.Partial("../Partial/Navigator")
    </div>
        </div>
<style>
    #a .gigantic.pagination {
        margin: 6px 5px 0px 40px;
