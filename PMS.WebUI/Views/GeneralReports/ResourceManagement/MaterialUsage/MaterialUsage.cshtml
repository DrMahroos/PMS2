                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtScope1RP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 col-xs-12">
                            <input type="radio" class="radio_resm" id="RAD_summary" name="status" />
                            <label class="control-label text-right" style="margin:0px 3px">@SystemResource.Spec_summary</label>
                        </div>
                        <div class="col-md-2 col-xs-12">
                            <input type="radio" class="radio_resm" id="RAD_Detail" name="status" />
                            <label class="control-label text-right" style="margin:0px 3px">@SystemResource.Spec_Detail</label>
