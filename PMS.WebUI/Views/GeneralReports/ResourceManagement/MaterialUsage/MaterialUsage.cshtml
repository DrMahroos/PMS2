                        <div class="col-md-2 col-sm-4 col-xs-5" style="width:25.5%">
                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtProject1RP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Eng_Rep_Dailyworkprod_customerclass</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="hidden" class="form-control input-sm" name="txtCustomerClassID" id="txtCustomerClassID">
                            <input type="text" class="form-control input-sm" disabled="disabled" id="txtCustomerClassRP" name="txtCustomerClassRP">
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" id="btnCustomerClassRP" name="btnCustomerClassRP">
                                <i class="glyphicon glyphicon-search"></i>
