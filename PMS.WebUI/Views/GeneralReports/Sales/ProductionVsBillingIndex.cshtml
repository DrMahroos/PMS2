@using PMS.WebUI.Resources;

<link href="~/css/main.css" rel="stylesheet" />

<div class="clear"></div>


<div class="row">
    <div class="col-xs-12 color1">
        
    </div>
    <div class="clear"></div>
    <label for="rpTitle" style="display:none" id="rpTitle">@SystemResource.Men_ProductionVsBilling</label>
    <form class="col-xs-12">
        <div class="col-xs-12">
            <div class="form-horizontal">

                <div class="form-group">
                    <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Fld_FromDate</label>
                    <div class="col-md-2 col-sm-4 col-xs-5">
                        <input type="date" class="form-control input-sm" id="FromDate" name="FromDate">
                    </div>
                    <label class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Fld_ToDate</label>
                    <div class="col-md-2 col-sm-4 col-xs-5">
                        <input type="date" class="form-control input-sm" id="ToDate" name="ToDate">
                    </div>
                    <div>
                        <label for="RDProjBlanc" class="col-md-6 col-sm-2 col-xs-12 control-label text-right sales_reportfirst"style="max-width:292px;margin-right:-8px"> @SystemResource.RDProjBlanc</label>
                        <input type="radio" name="RDProdcVsBill" id="RDProjBlanc" value="option1" checked="checked" class="category_input even_label">

                    </div>

                </div>
            </div>


            <div class="form-horizontal">
                <div class="form-group">
                    <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Fld_Project</label>
                    <div class="col-md-2 col-sm-4 col-xs-5">
                        <input type="text" class="form-control input-sm" id="txtFromProjCode" name="FromProjCode">
                    </div>
                    <label class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Fld_Project</label>
                    <div class="col-md-2 col-sm-4 col-xs-5" style="margin-right: 94px;">
                        <input type="text" class="form-control input-sm" id="txtToProjCode" name="ToProjCode">
                    </div>
                    <div>
                        <label for="RDProdcutList" class="col-md-6 col-sm-2 col-xs-12 control-label text-right sales_reportsecond"style="max-width:292px;margin-right:-9px"> @SystemResource.RDProdcutList</label>
                        <input type="radio" name="RDProdcVsBill" id="RDProdcutList" value="1" class="category_input even_label">

                    </div>
                </div>
            </div>
            <div class="form-horizontal">
                <div class="form-group">
                    <label class="col-md-2 col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Fld_SalesEngineerId </label>
                    <div class="col-md-1 col-sm-4 col-xs-5">
                        <input class="form-control input-sm " disabled="disabled" name="SalesEngineerId" id="txtSalesEngineerIdRP">
                        <input type="hidden" class="form-control input-sm " disabled="disabled" name="SalesEngineerId" id="txtSalesEngineerIdHRP">
                    </div>
                    <div class="info-btn">
                        <button type="button" class="src-btn btn btn-warning" id="butEngCodeRP">
                            <i class="glyphicon glyphicon-search"></i>
                        </button>
                    </div>
                    <div class="catin col-md-2 col-sm-4 col-xs-5"style="margin-right: 190px;">
                        <input type="text" class="form-control inpt-sm " disabled="disabled" id="txtDescECatRP">
                    </div>
                    <div>
                        <label for="RDByCustomer" class="col-md-6 col-sm-2 col-xs-12 control-label text-right sales_reportsecond"style="max-width:292px;margin-right:-8px"> @SystemResource.RDByCustomer</label>
                        <input type="radio" name="RDProdcVsBill" id="RDByCustomer" value="2" class="category_input even_label">

                    </div>
                </div>
            </div>
            <div class="form-horizontal">
                <div class="form-group">

                    <label class="col-md-2 col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Men_CustomerCategory </label>
                    <div class="col-md-1 col-sm-4 col-xs-5">
                        <input type="text" class="form-control input-sm " disabled="disabled" id="txtCustomercCodeRP" name="CustomerCategoryID">
                        <input type="hidden" class="form-control input-sm " disabled="disabled" id="txtCustomercIDRP" name="CustomerCategoryID">

                    </div>
                    <div class="info-btn">

                        <button type="button" class="btn btn-info" id="btnCustomerCatRP" name="btnCustomerCatRP">
                            <i class="glyphicon glyphicon-search"></i>
                        </button>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-5" style="width:21.5%;margin-right: 90px;">

                        <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtCustomerName1RP">
                    </div>
                    <div>
                        <label for="RDByProject" class="col-md-6 col-sm-2 col-xs-12 control-label text-right sales_reportfirst"style="max-width:292px;margin-right: -3px;"> @SystemResource.RDByProject</label>
                        <input type="radio" name="RDProdcVsBill" id="RDByProject" value="3" class="category_input even_label">

                    </div>

                </div>
            </div>

            <div class="form-horizontal">

                <div class="form-group">
                   
                    <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Def_customer </label>
                    <div class="col-md-1 col-sm-4 col-xs-5">
                        <input type="text" class="form-control input-sm" disabled="disabled" id="txtCustomerCodeRP" name="CustomerCode">
                        <input type="hidden" class="form-control input-sm" name="CustomerID" id="txtCustomerIDRP">
                    </div>
                    <div class="info-btn">
                        <button class="btn btn-info" type="button" id="btnCustomerCodeRP">
                            <i class="glyphicon glyphicon-search"></i>
                        </button>
                    </div>
                    <div class="col-md-2 col-sm-4 col-xs-5" style="width:21.5%;margin-right: 90px;">

                        <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtCustomerNameRP">
                    </div>

                    <div>
                        <label for="RDByEng" class="col-md-6 col-sm-2 col-xs-12 control-label text-right sales_reportfirst"style="max-width:292px;margin-right:-4px"> @SystemResource.RDByEng</label>
                        <input type="radio" name="RDProdcVsBill" id="RDByEng" value="4" class="category_input even_label">

                    </div>
                </div>

            </div>
            <div class="form-horizontal">
                <div class="form-group">
                  
                    <div style="margin-left:607px;">
                        <label for="RDCustBlanc" class="col-md-8 col-sm-2 col-xs-12 control-label text-right sales_reportfirst "style="max-width:292px;margin-right:-4px"> @SystemResource.RDCustBlanc</label>
                        <input type="radio" name="RDProdcVsBill" id="RDCustBlanc" value="option1" class="category_input even_label">

                    </div>
                </div>
            </div>



        </div>
    </form>

</div>

    
  <script src="~/ClientApp/Components/Sales/04_Reports/ProductionVsBilling.js"></script>


