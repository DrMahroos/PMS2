@using PMS.WebUI.Resources


<div class="clear"></div>
<label for="rpTitle" style="display:none" id="rpTitle" langRef="Rep_AttendanceAndLeaveS">@SystemResource.Resm_ProjectRequirements</label>
<div class="row">
    <div class="clear"></div>
    <div class="col-xs-12 dir" id="effects">
        <form>
            <div class="col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtEngCode" class="col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Resm_branch </label>
                        <div class="col-md-1 col-sm-4 col-xs-5" style="width: 130px;">
                            <input type="hidden" class="form-control input-sm" name="BranchID" id="txtBranchID">

                            <input type="text" class="form-control input-sm addable editable" style="width:;" name="BranchRP" disabled id="txtBranchRP">
                        </div>
                        <div class="info-btn col-md-1" style="width: 56px;">
                            <button type="button" class="src-btn btn btn-warning xaddable xeditable fst_serach_proj" id="btnBranchRP" name="BranchRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-3 col-sm-4 col-xs-5">
                            <input type="text" class="form-control input-sm addable editable" name="Branch1RP" disabled id="txtBranch1RP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtDescE" class="col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Fld_Year </label>
                        <div class="col-md-1 col-sm-4 col-xs-5" style="width: 181px;">
                            <input type="hidden" class="form-control input-sm" name="SubContractorID" id="txtSubContractorId">
                            <input type="text" class="form-control input-sm" name="YearID" id="txtYearID" disabled>
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtDescE" class="col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Eng_Budg_Month </label>
                        <div class="col-md-1 col-sm-4 col-xs-5" style="width: 181px;">
                            <select class="form-control" 1 id="txtMonthID">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                                <option>6</option>
                                <option>7</option>
                                <option>8</option>
                                <option>9</option>
                                <option>10</option>
                                <option>11</option>
                                <option>12</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="col-md-2 col-xs-12">
                            <input type="checkbox" class="radio_resm" name="status"id="IsLaborID" />
                            <label class="col-sm-2 col-xs-12 control-label text-right" style="width: 100px;">@SystemResource.Men_Labor</label>
                        </div>
                        <div class="col-md-2 col-xs-12">
                            <input type="checkbox" class="radio_resm" name="status" id="IsEquipID"/>
                            <label class="col-sm-2 col-xs-12 control-label text-right" style="width: 100px;">@SystemResource.sub_equipment</label>
                        </div>
                        <div class="col-md-2 col-xs-12">
                            <input type="checkbox" class="radio_resm" name="status" id="IsMatID"/>
                            <label class="col-sm-2 col-xs-12 control-label text-right" style="width: 100px;">@SystemResource.Act_ItemCode</label>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
<script src="~/ClientApp/Components/Engineer/Budget/BudgetRequirements.js"></script>