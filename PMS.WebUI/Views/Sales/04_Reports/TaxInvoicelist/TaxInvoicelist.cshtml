@using PMS.WebUI.Resources;
<style>
    .col-1 {
        padding-left: 10px !important;
        padding-right: 10px !important
    }
    input[type=radio] {
        margin: -1px 0 0;
    }
</style>
<div class="row">
    <div class="col-xs-12 color1"> 
    </div>
    <div class="clear"></div>
    <label for="rpTitle" style="display:none" id="rpTitle">@SystemResource.TaxInvoicelist</label>
    <div class="col-xs-12 dir" id="effects">
        <div style="height:5px"></div>
        <div class="clear"></div>
        <form class="col-xs-12">
            <div class="col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Fld_FromDate</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="date" class="form-control input-sm" id="FromDate" name="FromDate">
                        </div>
                        <label class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Fld_ToDate</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="date" class="form-control input-sm" id="ToDate" name="ToDate">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">From @SystemResource.Eng_Fld_projectexpproject</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="hidden" class="form-control input-sm" name="txtProjectID" id="txtProjectID">
                            <input type="text" class="form-control input-sm" disabled="disabled" id="txtProjectRP" name="txtProjectRP">
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" id="btnProjectRP" name="btnProjectRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtProject1RP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">To @SystemResource.Eng_Fld_projectexpproject</label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="hidden" class="form-control input-sm" name="txttoProjectID" id="txttoProjectID">
                            <input type="text" class="form-control input-sm" disabled="disabled" id="txttoProjectRP" name="txttoProjectRP">
                        </div>
                        <div class="info-btn">
                            <button type="button" class="btn btn-warning" id="btntoProjectRP" name="btntoProjectRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txttoProject1RP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-md-2 col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Fld_SalesEngineerId </label>
                        <div class="col-md-2 col-sm-2 col-xs-2">
                            <input class="form-control input-sm " disabled="disabled" name="SalesEngineerId" id="txtSalesEngineerIdRP">
                            <input type="hidden" class="form-control input-sm " disabled="disabled" name="SalesEngineerId" id="txtSalesEngineerIdHRP">
                        </div>
                        <div class="info-btn">
                            <button type="button" class="src-btn btn btn-warning" id="butEngCodeRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <input type="text" class="form-control inpt-sm " disabled="disabled" id="txtDescECatRP">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Def_customer </label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="text" class="form-control input-sm" disabled="disabled" id="txtCustomerCode" name="CustomerCode">
                            <input type="hidden" class="form-control input-sm" name="CustomerID" id="txtCustomerID">
                        </div>
                        <div class="info-btn">
                            <button class="btn btn-info" type="button" id="btnCustomer">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtCustomerName">
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.Men_CustomerCategory </label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="text" class="form-control input-sm" disabled="disabled" id="txtCustomerCodeRP" name="CustomerCode">
                            <input type="hidden" class="form-control input-sm" name="CustomerID" id="txtCustomerIDRP">
                        </div>
                        <div class="info-btn">
                            <button class="btn btn-info" type="button" id="btnCustomerCodeRP">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-5 col-sm-5 col-xs-5">
                            <input type="text" class="form-control inpt-sm" disabled="disabled" id="txtCustomerNameRP">
                        </div>
                    </div>
                </div>


                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 col-xs-12 control-label text-right">@SystemResource.ChkStatus </label>
                        <div class="col-md-1 col-sm-4 col-xs-5 col-1">
                            <label  class="col-xs-8 control-label" style=" padding-left:0px;padding-right:0px"> Progress </label>
                            <input type="radio" checked class=" col-xs-4" name="1" id="redProgress">
                        </div>
                        <div class="col-md-2 col-sm-4 col-xs-5 col-1">
                            <label class="col-xs-8 control-label" style=" padding-left:0px;padding-right:0px"> Down payment </label>
                            <input type="radio" class="col-xs-2" name="1" id="redDownpayment">
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
 </div> 
<script src="~/ClientApp/Components/Sales/04_Reports/TaxInvoicelist.js"></script>