				<div class="col-xs-12 form-horizontal" id="defination">
					<style>
                        .jsgrid-grid-body {
                            height: 200px !important;
                            background-color: #ddd;
                            margin-top: -14px;
                        }
                        .form-group {
                            margin-top: 2px;
                        }
					</style>

					<table id="sub_definations"></table>
				</div>
			</div>
		</form>
	</div>
	<div>
		<table></table>
	</div>

	<script src="~/ClientApp/JsGrid.js"></script>
	@Html.Partial("../Partial/Navigator")
