                            </div>
						</div>
					</div>
				</div>


				<div class="form-horizontal">
					<div class="form-group">
						<label for="txtDescE" class="col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Pre_Tele1 </label>
						<div class="col-sm-4 col-xs-12">
							<input type="text" maxlength="20" class="form-control input-sm addable editable" name="Tel1" id="txtTel1" />
						</div>
						<div class="col-md-1"></div>
						<div class="col-md-6 col-sm-2 col-xs-6 ">
							<label for="txtEngCode" class="col-sm-3 col-xs-12 control-label text-right"> @SystemResource.Pre_vatno </label>
