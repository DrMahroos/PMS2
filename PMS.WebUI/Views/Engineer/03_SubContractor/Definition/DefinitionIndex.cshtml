						<label for="txtDescE" class="col-sm-2 col-xs-12 control-label text-right"> @SystemResource.Pre_company_namea </label>
						<div class="col-sm-4 col-xs-12">
							<input type="text" maxlength="100" class="form-control input-sm addable editable" name="DescA" id="txtDescA" />
						</div>
						<div class="col-md-1"></div>
						<div class="col-md-6 col-sm-2 col-xs-6 ">
