@using PMS.WebUI.Resources
<div class="row">
    <div class="clear"></div>
    <div class="col-xs-12 dir" id="effects">


        <div style="height:5px"></div>
        <div class="clear"></div>
        <form>
            <div class="col-xs-12">

                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtEngCode" class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Fld_FromDate </label>
                        <div class="col-md-2 col-sm-4 col-xs-5">
                            <input type="date" class="form-control input-sm addable editable" name="FromDate" id="FromDate">
                            <input type="hidden" name="ProjID" id="txtProjID">
                            <input type="hidden" name="PhaseID" id="txtPhaseID">


                        </div>
                        <label for="txtEngCode" class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Fld_ToDate </label>
                        <div class="col-md-2 col-sm-4 col-xs-5" style="width: 173px;">
                            <input type="date" class="form-control input-sm addable editable" style="width:;" name="ToDate" id="ToDate">
                        </div>
                    </div>
                </div>


                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtEngCode" class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Eng_Res_projectexpProject </label>
                        <div class="col-md-1 col-sm-4 col-xs-5" style="width: 119px;">
                            <input type="text" class="form-control input-sm addable editable" style="width:;" name="txtProjCode" id="txtProjCode">
                        </div>
                        <div class="info-btn col-md-1" style="width: 54px;">
                            <button type="button" class="src-btn btn btn-warning xaddable xeditable fst_serach_proj" id="btnProjCode" name="btnProjCode">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-3 col-sm-4 col-xs-5">
                            <input type="text" class="form-control input-sm addable editable" style="width:;" name="txtProjDesc" id="txtProjDesc" disabled>
                        </div>

                        <div class="col-md-2" style="width: 118px;">
                            <label for="byactivity" style="margin: 4px;">By Activity</label>
                            <input type="radio" name="item" class="progress_input" id="byactivity" />
                        </div>
                    </div>
                </div>


                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="txtEngCode" class="col-sm-1 col-xs-12 control-label text-right"> @SystemResource.Eng_Res_projectexpPhase </label>
                        <div class="col-md-1 col-sm-4 col-xs-5" style="width: 119px;">
                            <input type="text" class="form-control input-sm addable editable" style="width:;" name="txtPhaseCode" id="txtPhaseCode" disabled>
                        </div>
                        <div class="info-btn col-md-1" style="width: 54px;">
                            <button type="button" class="src-btn btn btn-warning xaddable xeditable fst_serach_proj" id="btnPhaseCode" name="btnPhaseCode">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                        <div class="col-md-3 col-sm-4 col-xs-5">
                            <input type="text" class="form-control input-sm addable editable" style="width:;" name="txtPhaseDesc" id="txtPhaseDesc" disabled>
                        </div>

                        <div class="col-md-2" style="width: 101px;">
                            <label for="byprojitem" style="margin: 4px;">By Item</label>
                            <input type="radio" name="item" class="progress_input" id="byprojitem" />
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
    <div>
        <table></table>
    </div>
</div>
<script src="~/ClientApp/Components/Engineer/05_Reports/ProjectProgress.js"></script>


